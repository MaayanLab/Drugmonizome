{
	"$validator": "/dcic/signature-commons-schema/v5/core/schema.json",
	"meta": {
		"match": {
			"${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/signature.json"
		},
		"$validator": "/dcic/signature-commons-schema/v5/meta/schema/ui-schema.json",
        "properties": {
            "Image": {
                "visibility": 1,
                "type": "img",
                "src": "${library.meta['Icon']}",
                "alt": "${library.meta['Library_name']}",
                "priority": 1,
                "icon": true
            },
            "Library": {
                "visibility": 1,
                "type": "text",
                "text": "${library.meta['Library_name']}",
                "priority": 1,
                "subtitle": true
            },
            "Association Term": {
                "visibility": 1,
                "type": "text",
                "text": "${meta['Term'][0]['Name']}",
                "name": true,
                "priority": 1
            },
            "Association Type": {
                "visibility": 1,
                "type": "text",
                "MDI_Icon":"mdi-ray-start-end",
                "text": "${meta['Association_Type']}",
                "priority": 2
            },
            "P-Value": {
                "visibility": 1,
                "condition": {
                    "${meta['p-value']}": {
                        "ne": null
                    }
                },
                "columnType": "number",
                "columnOptions": {
                    "sort": true,
                    "sortDirection": "asc"
                },
                "type": "text",
                "text": "${meta['p-value']}",
                "sortable": true,
                "score": true,
                "priority": 8
            },
            "Odds Ratio": {
                "visibility": 1,
                "condition": {
                    "${meta.oddsratio}": {
                        "ne": null
                    }
                },
                "columnType": "number",
                "type": "text",
                "text": "${meta.oddsratio}",
                "sortable": true,
                "score": true,
                "priority": 9
            },
            "Drug Set Size": {
                "visibility": 1,
                "condition": {
                    "${meta.setsize}": {
                        "ne": null
                    }
                },
                "columnType": "number",
                "type": "text",
                "text": "${meta.setsize}",
                "sortable": true,
                "score": true,
                "priority": 7
            }
        }
    }
}