{
    "$validator": "/dcic/signature-commons-schema/v5/core/schema.json",
        "meta": {
            "type": "entity",
            "match": {
                "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/entity.json"
            },
            "$validator": "/dcic/signature-commons-schema/v5/meta/schema/ui-schema.json",
            "properties": {
                "Name": {
                    "name": true,
                    "text": "${meta.Name}",
                    "type": "text",
                    "field": "meta.Name",
                    "priority": 1,
                    "visibility": 1
                },
                "InChI_key": {
                    "text": "${meta.InChI_key}",
                    "type": "text",
                    "field": "meta.InChI_key",
                    "priority": 2,
                    "visibility": 2
                },
                "Synonyms": {
                    "text": "${meta.Synonyms}",
                    "type": "text",
                    "field": "meta.Synonyms",
                    "priority": 3,
                    "synonyms": true,
                    "visibility": 1
                },
                "SMILES": {
                    "text": "${meta.SMILES}",
                    "type": "text",
                    "field": "meta.SMILES",
                    "priority": 3,
                    "visibility": 2
                }
            }
        }
    }