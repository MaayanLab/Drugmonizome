{
	"$validator": "/dcic/signature-commons-schema/v5/core/schema.json",
	"meta": {
		"match": {
			"${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/library.json"
		},
		"$validator": "/dcic/signature-commons-schema/v5/meta/schema/ui-schema.json",
		"properties": {
			"Image": {
				"alt": "${meta['Library_name']}",
				"src": "${meta['Icon']}",
				"icon": true,
				"type": "img",
				"priority": 1,
				"visibility": 1
			},
			"Library": {
				"name": true,
				"text": "${meta['Library_name']}",
				"type": "text",
				"priority": 1,
				"visibility": 1
			},
			"Description": {
				"text": "${meta['Description']}",
				"type": "text",
				"priority": 2,
				"subtitle": true,
				"visibility": 1,
				"description": true,
				"MDI_Icon": "mdi-flask-outline"
			},
			"Association Type": {
				"text": "${meta['Association_Type']}",
				"type": "text",
				"priority": 2,
				"visibility": 1,
				"MDI_Icon": "mdi-flask-outline"

			}
		}
	}
}